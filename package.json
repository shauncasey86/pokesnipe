{
  "name": "pokesnipe",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node migrate.mjs && node dist/server/index.js",
    "start:worker": "node dist/server/worker.js",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.server.json --noEmit && tsc -p tsconfig.client.json --noEmit",
    "migrate": "node-pg-migrate -m migrations -d $DATABASE_URL",
    "migrate:up": "node-pg-migrate -m migrations -d $DATABASE_URL up",
    "migrate:down": "node-pg-migrate -m migrations -d $DATABASE_URL down"
  },
  "dependencies": {
    "axios": "1.7.7",
    "cookie": "0.6.0",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "pg": "8.12.0",
    "pino": "9.3.2",
    "pino-http": "9.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "uuid": "10.0.0",
    "node-pg-migrate": "7.6.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cookie": "0.6.0",
    "@types/express": "4.17.21",
    "@types/node": "20.14.15",
    "@types/pg": "8.11.6",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "4.3.1",
    "concurrently": "9.0.1",
    "eslint": "9.9.1",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "vite": "5.4.3"
  }
}
